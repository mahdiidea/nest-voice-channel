    "reflect-metadata": "^0.1.13",
    "resolve-url-loader": "^5.0.0",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sass": "^1.50.0",
    "sass-loader": "^12.6.0",
