    "crypto-js": "^4.1.1",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.0.3",
    "moment": "^2.29.2",
    "moment-jalaali": "^0.9.4",
